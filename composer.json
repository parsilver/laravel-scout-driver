{
    "name": "farzai/laravel-scout-driver",
    "type": "library",
    "license": "MIT",
    "description": "More scout driver for laravel",
    "keywords": [
        "laravel", "scout"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "laravel/scout": "^9.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\ScoutDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "PA",
            "role": "DevOps"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Farzai\\ScoutDriver\\ScoutDriverServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
